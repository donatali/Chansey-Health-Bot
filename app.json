{
  "expo": {
    "name": "react_native_jobs",
    "slug": "react_native_jobs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/Chansey.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abrahamvegaa.react-native-jobs"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/Chansey.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abrahamvegaa.react_native_jobs"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/Chansey.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/Chansey.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
